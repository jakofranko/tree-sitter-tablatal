{
  "name": "Tablatal",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "comment_definition"
          }
        },
        {
          "type": "SYMBOL",
          "name": "header_definition"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "row_definition"
          }
        }
      ]
    },
    "comment_definition": {
      "type": "PATTERN",
      "value": "~\\s?.*"
    },
    "header_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "column_definition"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "column_definition": {
      "type": "PATTERN",
      "value": "[A-Z\\_]+"
    },
    "row_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "item_definition"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "item_definition": {
      "type": "PATTERN",
      "value": "\\S+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
